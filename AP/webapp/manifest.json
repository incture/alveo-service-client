{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.menabev.AP",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.menabev.AP.view.App",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.menabev.AP.i18n.i18n"
				}
			},
			"StaticDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oUserDetailModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oMandatoryModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oVisibilityModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oDropDownModel": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.menabev.AP.view",
				"controlAggregation": "pages",
				"controlId": "AppId",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "UploadInvoice",
				"pattern": "UploadInvoice",
				"target": [
					"UploadInvoice"
				]
			}, {
				"name": "UserManagement",
				"pattern": "",
				"target": [
					"UserManagement"
				]
			}, {
				"name": "Inbox",
				"pattern": "Inbox",
				"target": [
					"Inbox"
				]
			}, {
				"name": "CreateUser",
				"pattern": "CreateUser/{id}",
				"target": [
					"CreateUser"
				]
			}, {
				"name": "CreateGroup",
				"pattern": "CreateGroup/{id}",
				"target": [
					"CreateGroup"
				]
			}, {
				"name": "ConfigCockpit",
				"pattern": "ConfigCockpit",
				"target": [
					"ConfigCockpit"
				]
			}, {
				"name": "TemplateManagement",
				"pattern": "TemplateManagement",
				"target": [
					"TemplateManagement"
				]
			},
			{
				"name": "NonPOInvoice",
				"pattern": "NonPOInvoice/:id:/:status:",
				"target": [
					"NonPOInvoice"
				]
			}],
			"targets": {
				"UploadInvoice": {
					"viewType": "XML",
					"viewName": "UploadInvoice"
				},
				"NonPOInvoice": {
					"viewType": "XML",
					"viewName": "NonPOInvoice"
				},
				"CreateUser": {
					"viewType": "XML",
					"viewName": "CreateUser"
				},
				"UserManagement": {
					"viewType": "XML",
					"viewName": "UserManagement"
				},
				"Inbox":{
					"viewType": "XML",
					"viewName": "Inbox"
				},
				"CreateGroup": {
					"viewType": "XML",
					"viewName": "CreateGroup"
				},
				"ConfigCockpit": {
					"viewType": "XML",
					"viewName": "ConfigCockpit"
				},
				"TemplateManagement": {
					"viewType": "XML",
					"viewName": "TemplateManagement"
				}
			}
		}
	}
}